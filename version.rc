 FILEOS 0x4L
 FILETYPE 0x0L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "080404b0"
        BEGIN
            VALUE "Comments", MY_VERSION_COMMENTS
            VALUE "CompanyName", MY_VERSION_COMPANYNAME
            VALUE "FileDescription", MY_VERSION_FILEDESCRIPTION
            VALUE "FileVersion", MY_VERSION_FILEVERSION
