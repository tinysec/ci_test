            VALUE "ProductName", MY_VERSION_PRODUCTNAME
            VALUE "ProductVersion", MY_VERSION_PRODUCTVERSION
            VALUE "SpecialBuild", MY_VERSION_SPECIALBUILD
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x804, 1200
    END
END

#else

